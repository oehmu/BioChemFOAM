FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      Ci;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{
    movingWall
    {
        type            zeroGradient;
        //type            symmetryPlane;
    }

    fixedWalls
    {
        type            zeroGradient;
        //type            symmetryPlane;
    }

    frontAndBack
    {
        type            empty;
    }
}


